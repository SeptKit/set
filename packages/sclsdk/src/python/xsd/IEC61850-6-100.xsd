			<xs:documentation>Log control parameters for service specification</xs:documentation>
			<xs:appinfo>Definition in IEC 61850-90-30</xs:appinfo>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="eIEC61850-6-100:tControlBlockParameters">
				<xs:attribute name="intgPd" type="xs:unsignedInt" use="optional">
					<xs:annotation>
						<xs:documentation>value to use for the intgPd</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="logName" type="scl:tLogName" use="optional">
					<xs:annotation>
						<xs:documentation>name of the Log to use</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="logLdInst" type="scl:tLDInst" use="optional">
