	<xs:complexType name="tFunctionRoleContent">
		<xs:annotation>
			<xs:documentation>
			Definition of the content of a Function Role
			In case of a content which can be instantiated multiple times when Cardinality is greater than 1, then this content will be instantiated multiple times
			</xs:documentation>
			<xs:appinfo>Definition in IEC 61850-7-6</xs:appinfo>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="eIEC61850-6-100:tBaseExtensionElement">
				<xs:sequence>
					<xs:choice minOccurs="0" maxOccurs="unbounded">
						<xs:element name="FunctionRef" type="eIEC61850-6-100:tFunctionRef">
							<xs:annotation>
								<xs:documentation>Reference to a function definition attached to current role</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="BehaviorDescriptionRef" type="eIEC61850-6-100:tBehaviorDescriptionRef">
							<xs:annotation>
								<xs:documentation>Reference to a behavior description attached to current role</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="ProcessResourceRef" type="eIEC61850-6-100:tProcessResourceRef">
							<xs:annotation>
								<xs:documentation>Reference to a process resource attached to current role</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="VariableRef" type="eIEC61850-6-100:tVariableRefWithUuid">
							<xs:annotation>
								<xs:documentation>Reference to a variable attached to current role</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="FunctionCategoryRef" type="eIEC61850-6-100:tFunctionCategoryRef">
							<xs:annotation>
								<xs:documentation>Reference to a function category attached to current role</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="PowerSystemRelationRef" type="eIEC61850-6-100:tPowerSystemRelationRef">
							<xs:annotation>
								<xs:documentation>Reference to a power system relation attached to current role</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:choice>
				</xs:sequence>
				<xs:attribute name="roleInst" use="optional" default="1">
					<xs:annotation>
						<xs:documentation>Instance of the role</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:integer">
							<xs:minInclusive value="1"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:attribute>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="tFunctionRole">
		<xs:annotation>
			<xs:documentation>Definition of the Function Role element</xs:documentation>
			<xs:appinfo>Definition in IEC 61850-7-6</xs:appinfo>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="eIEC61850-6-100:tFunctionalVariantRefContainer">
				<xs:sequence>
					<xs:choice minOccurs="1" maxOccurs="unbounded">
						<xs:element name="FunctionRoleContent" type="eIEC61850-6-100:tFunctionRoleContent">
							<xs:annotation>
								<xs:documentation>an instance of the current role. In case of application type definition, instance 1 is the default instance</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:choice>
				</xs:sequence>
				<xs:attribute name="name" type="scl:tName" use="required">
					<xs:annotation>
						<xs:documentation>Name of the role</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attributeGroup ref="eIEC61850-6-100:agUuidWithOrigin"/>
				<xs:attribute name="type" type="xs:normalizedString">
					<xs:annotation>
						<xs:documentation>Type of the role (Predefined value = EXTERNAL)</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attributeGroup ref="eIEC61850-6-100:agCardinalityWithSelector">
					<xs:annotation>
						<xs:documentation>
						Selector is used to give a searching rule to help user to select target(s) to deploy the given role. 
						When the target is obvious it could even more be automatically deployed.
						When a selector is defined, the user may select different candidates matching this selector, and then the FunctionRole 
						
						The cardinality, indicated the rule to instanciate the role multiple times. 
						</xs:documentation>
					</xs:annotation>
				</xs:attributeGroup>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
