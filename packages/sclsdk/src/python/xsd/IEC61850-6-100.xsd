					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="buffered" type="xs:boolean" use="optional">
					<xs:annotation>
						<xs:documentation>value to use for the buffered</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="bufTime" type="xs:unsignedInt" use="optional">
					<xs:annotation>
						<xs:documentation>value to use for the bufTime</xs:documentation>
					</xs:annotation>
				</xs:attribute>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="tLogParameters">
