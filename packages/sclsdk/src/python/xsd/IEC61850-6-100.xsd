					<xs:annotation>
						<xs:documentation>UUID of the power system relation referenced</xs:documentation>
					</xs:annotation>
				</xs:attribute>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<!-- ********************* -->
	<!-- Allocation Role management -->
	<!-- ********************* -->
	<xs:complexType name="tAllocationRole">
		<xs:annotation>
			<xs:documentation>Definition of the Allocation Role element</xs:documentation>
			<xs:appinfo>Definition in IEC 61850-90-30</xs:appinfo>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="eIEC61850-6-100:tBaseExtensionElementWithDesc">
				<xs:choice minOccurs="0" maxOccurs="unbounded">
					<xs:element name="FunctionRef" type="eIEC61850-6-100:tFunctionRef" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>Reference to a function definition attached to current allocation role</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:choice>
				<xs:attribute name="name" type="scl:tName" use="required">
					<xs:annotation>
						<xs:documentation>Name of the role</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attributeGroup ref="eIEC61850-6-100:agUuidWithOrigin"/>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:element name="AllocationRole" type="eIEC61850-6-100:tAllocationRole">
		<xs:annotation>
			<xs:documentation>Definition of an allocation role allowing definition of possible allocation to devices type without using virtual IED</xs:documentation>
			<xs:appinfo>Definition in IEC 61850-90-30</xs:appinfo>
		</xs:annotation>
	</xs:element>
	<!-- ********************** -->
	<!-- Functional variants   -->
	<!-- ********************** -->
	<xs:complexType name="tAbstractFunctionalVariant" abstract="true">
		<xs:annotation>
			<xs:documentation>Definition of a functional variant of the Application</xs:documentation>
			<xs:appinfo>Definition in IEC 61850-7-6</xs:appinfo>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="eIEC61850-6-100:tBaseExtensionElementWithDesc">
				<xs:choice minOccurs="0" maxOccurs="unbounded">
					<xs:element name="FunctionalSubVariant" type="eIEC61850-6-100:tFunctionalSubVariant" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>Sub variant of the current variant</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="VariableRef" type="eIEC61850-6-100:tVariableRefWithUuid" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>Reference to a variable attached to current variant, to parameterize the instance with the value defined in the reference</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:choice>
				<xs:attribute name="name" type="scl:tName" use="required">
					<xs:annotation>
						<xs:documentation>Name of the variant</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attributeGroup ref="eIEC61850-6-100:agUuidWithOrigin"/>
				<xs:attribute name="isBaseline" type="xs:boolean" use="optional" default="false">
					<xs:annotation>
						<xs:documentation>Indication if the functional variant is used as baseline variant used as reference for other variants</xs:documentation>
					</xs:annotation>
				</xs:attribute>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="tFunctionalVariant">
		<xs:annotation>
			<xs:documentation>Definition of a functional variant of the Application</xs:documentation>
			<xs:appinfo>Definition in IEC 61850-7-6</xs:appinfo>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="eIEC61850-6-100:tAbstractFunctionalVariant"/>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="tFunctionalSubVariant">
		<xs:annotation>
			<xs:documentation>Definition of a functional sub variant of a variant</xs:documentation>
			<xs:appinfo>Definition in IEC 61850-7-6</xs:appinfo>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="eIEC61850-6-100:tAbstractFunctionalVariant"/>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="tFunctionalVariantGroup">
		<xs:annotation>
			<xs:documentation>
			Definition of a group of functional variant which are exclusive to each other. 
			But an application may contains multiple groups which can be combined together
			</xs:documentation>
			<xs:appinfo>Definition in IEC 61850-7-6</xs:appinfo>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="eIEC61850-6-100:tBaseExtensionElementWithDesc">
				<xs:sequence>
					<xs:element name="FunctionalVariant" type="eIEC61850-6-100:tFunctionalVariant" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>Varian of the current group</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
				<xs:attribute name="name" type="scl:tName" use="required">
					<xs:annotation>
						<xs:documentation>Name of the group of variant</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attributeGroup ref="eIEC61850-6-100:agUuidWithOrigin"/>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="tFunctionalVariantRef">
		<xs:annotation>
			<xs:documentation>Definition of a ref to a functional variant of the Application</xs:documentation>
			<xs:appinfo>Definition in IEC 61850-7-6</xs:appinfo>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="eIEC61850-6-100:tBaseExtensionElement">
				<xs:attribute name="functionalVariant" type="scl:tName" use="optional">
					<xs:annotation>
						<xs:documentation>Name of the variant</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="functionalVariantUuid" type="scl:tUUIDAttribute" use="optional">
					<xs:annotation>
						<xs:documentation>UUID of the variant</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="update" use="optional" default="add">
					<xs:annotation>
						<xs:documentation>kind of update to apply to the variant</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:enumeration value="add"/>
							<xs:enumeration value="remove"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:attribute>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="tFunctionalVariantRefContainer" abstract="true">
		<xs:annotation>
			<xs:documentation>Type which could be linked to functional variants</xs:documentation>
			<xs:appinfo>Definition in IEC 61850-7-6</xs:appinfo>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="eIEC61850-6-100:tBaseExtensionElementWithDesc">
				<xs:choice minOccurs="0" maxOccurs="unbounded">
					<xs:element name="FunctionalVariantRef" type="eIEC61850-6-100:tFunctionalVariantRef" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>Ref to a functional variant of the Application</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:choice>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<!-- ************** -->
	<!-- Application   -->
	<!-- ************** -->
	<xs:complexType name="tApplication">
		<xs:annotation>
			<xs:documentation>Definition of the Application element</xs:documentation>
			<xs:appinfo>Definition in IEC 61850-7-6</xs:appinfo>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="eIEC61850-6-100:tBaseExtensionElementWithDesc">
				<xs:sequence>
					<xs:element name="FunctionRole" type="eIEC61850-6-100:tFunctionRole" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>Role attached to the application type</xs:documentation>
						</xs:annotation>
						<xs:unique name="uniqueRoleContent">
							<xs:selector xpath="./eIEC61850-6-100:FunctionRoleContent"/>
							<xs:field xpath="@roleInst"/>
						</xs:unique>
					</xs:element>
					<xs:element name="FunctionalVariant" type="eIEC61850-6-100:tFunctionalVariant" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>Variant attached to the application type</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="FunctionalVariantGroup" type="eIEC61850-6-100:tFunctionalVariantGroup" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>Group of variants attached to the application type</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="AllocationRoleRef" type="eIEC61850-6-100:tAllocationRoleRef" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>Allocation role attached to the application type</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="ApplicationSclRef" type="eIEC61850-6-100:tApplicationSclRef" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Reference to the ASD files used to create an Application</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
				<xs:attribute name="type" type="scl:tID" use="optional">
					<xs:annotation>
						<xs:documentation>Identification of the application type if any</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="name" type="scl:tName" use="required">
					<xs:annotation>
						<xs:documentation>Name of the application</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attributeGroup ref="eIEC61850-6-100:agUuidWithOrigin"/>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:element name="Application" type="eIEC61850-6-100:tApplication">
		<xs:annotation>
			<xs:documentation>Application definition to be added at Process, Substation, Voltage Level or Bay level</xs:documentation>
			<xs:appinfo>Definition in IEC 61850-7-6</xs:appinfo>
		</xs:annotation>
	</xs:element>
	<!-- ************************* -->
	<!-- Behavior Description   -->
	<!-- ************************* -->
	<xs:simpleType name="tPredefinedBehaviorDescTypeEnum">
		<xs:restriction base="xs:normalizedString">
			<xs:enumeration value="IEC 61131"/>
			<xs:enumeration value="Textual"/>
			<xs:enumeration value="Graphic"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="tExtensionBehaviorDescTypeEnum">
		<xs:restriction base="xs:normalizedString">
			<xs:maxLength value="64"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="tBehaviorDescTypeEnum">
		<xs:union memberTypes="eIEC61850-6-100:tPredefinedBehaviorDescTypeEnum eIEC61850-6-100:tExtensionBehaviorDescTypeEnum"/>
	</xs:simpleType>
	<xs:complexType name="tLogicVarRef">
		<xs:annotation>
			<xs:documentation>A binding between a variable in the behavior description and a specification data object in the behavior description hierarchy.</xs:documentation>
			<xs:appinfo>Definition in IEC 61850-90-30</xs:appinfo>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="eIEC61850-6-100:tBaseExtensionElementWithDesc">
				<xs:attribute name="value">
					<xs:annotation>
						<xs:documentation>In case of internal variable (not linked to a data or an input or output) a default value can be defined</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:normalizedString">
							<xs:minLength value="1"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:attribute>
				<xs:attribute name="dataName" type="eIEC61850-6-100:tPathName" use="optional">
					<xs:annotation>
						<xs:documentation>local path to a DO or DA to be used in the logic (as input or output)</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="lnodeUuid" type="scl:tUUIDAttribute" use="optional">
					<xs:annotation>
						<xs:documentation>UUID of the LNode which contain the data to be used in the logic (as input or output)</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="doName" type="scl:tFullDOName" use="optional">
					<xs:annotation>
						<xs:documentation>Name of the DO to be used in the logic (as input or output) when lnodeUuid is defined</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="daName" type="scl:tFullAttributeName" use="optional">
					<xs:annotation>
						<xs:documentation>Name of the DA to be used in the logic (as input or output) when lnodeUuid is defined</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="varName" type="xs:normalizedString" use="required">
					<xs:annotation>
						<xs:documentation>variable name used in the logic</xs:documentation>
					</xs:annotation>
				</xs:attribute>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="tInputVar">
		<xs:annotation>
			<xs:documentation>A binding between a variable in the behavior description and a specification data object or input in the behavior description hierarchy.</xs:documentation>
			<xs:appinfo>Definition in IEC 61850-90-30</xs:appinfo>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="eIEC61850-6-100:tLogicVarRef">
				<xs:attribute name="inputName" type="eIEC61850-6-100:tPathName" use="optional">
					<xs:annotation>
						<xs:documentation>local path to an LNode SourceRef to be used as input of the logic</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="inputUuid" type="scl:tUUIDAttribute" use="optional">
					<xs:annotation>
						<xs:documentation>UUID of the LNode SourceRef to be used as input of the logic</xs:documentation>
					</xs:annotation>
				</xs:attribute>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="tOutputVar">
		<xs:annotation>
			<xs:documentation>A binding between a variable in the behavior description and a specification data object or output in the behavior description hierarchy.</xs:documentation>
			<xs:appinfo>Definition in IEC 61850-90-30</xs:appinfo>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="eIEC61850-6-100:tLogicVarRef">
				<xs:attribute name="outputName" type="eIEC61850-6-100:tPathName" use="optional">
					<xs:annotation>
						<xs:documentation>local path to an LNode ControlRef to be used as output of the logic</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="outputUuid" type="scl:tUUIDAttribute" use="optional">
					<xs:annotation>
						<xs:documentation>UUID of the LNode ControlRef to be used as output of the logic</xs:documentation>
					</xs:annotation>
				</xs:attribute>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="tBehaviorReference">
		<xs:annotation>
			<xs:documentation>A reference to another behavior description, used when a vendor is describing his implementation of a specific specification.</xs:documentation>
			<xs:appinfo>Definition in IEC 61850-90-30</xs:appinfo>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="eIEC61850-6-100:tBaseExtensionElementWithDesc">
				<xs:attribute name="behaviorReference" type="eIEC61850-6-100:tPathName" use="optional">
					<xs:annotation>
						<xs:documentation>path to another BehaviorDescription</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="behaviorUuid" type="scl:tUUIDAttribute" use="optional">
					<xs:annotation>
						<xs:documentation>UUID of another BehaviorDescription</xs:documentation>
					</xs:annotation>
				</xs:attribute>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="tBehaviorDescription">
		<xs:annotation>
			<xs:documentation>Definition of the BehaviorDescription element</xs:documentation>
			<xs:appinfo>Definition in IEC 61850-90-30</xs:appinfo>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="eIEC61850-6-100:tBaseExtensionElementWithDesc">
				<xs:sequence>
					<xs:element name="InputVar" type="eIEC61850-6-100:tInputVar" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>Input variable used in the behavior description</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="OutputVar" type="eIEC61850-6-100:tOutputVar" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>Output variable used in the behavior description</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="BehaviorReference" type="eIEC61850-6-100:tBehaviorReference" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>Reference to a behavior description when for example a manufacturer wants to indicate its implementation of a user specification described by another behavior description</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
				<xs:attribute name="name" use="required">
					<xs:annotation>
						<xs:documentation>Name of the behavior description.</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:normalizedString">
							<xs:minLength value="1"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:attribute>
				<xs:attribute name="format" type="eIEC61850-6-100:tBehaviorDescTypeEnum" use="optional">
					<xs:annotation>
						<xs:documentation>Identification of the behavior description format</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="fileReference" type="xs:anyURI" use="optional">
					<xs:annotation>
						<xs:documentation>Path to the behavior description file</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="isSpecification" type="xs:boolean" default="true">
					<xs:annotation>
						<xs:documentation>indication that the behavior description is used for specification</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="isSimulation" type="xs:boolean" default="false">
					<xs:annotation>
						<xs:documentation>indication that the behavior description is used for simulation</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attributeGroup ref="eIEC61850-6-100:agUuidWithOrigin"/>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:element name="BehaviorDescription" type="eIEC61850-6-100:tBehaviorDescription">
		<xs:annotation>
			<xs:documentation>Element allowing to describe the behavior its parent (Application, Bay, Function, LN...)</xs:documentation>
			<xs:appinfo>Definition in IEC 61850-90-30</xs:appinfo>
		</xs:annotation>
		<xs:unique name="uniqueChildNameInBehaviorDescription">
			<xs:selector xpath="./*"/>
			<xs:field xpath="@varName"/>
		</xs:unique>
	</xs:element>
	<!-- ******************* -->
	<!-- Project Definition -->
	<!-- ******************* -->
	<xs:complexType name="tProjectProcessReference">
		<xs:annotation>
			<xs:documentation>A reference to another behavior description, used when a vendor is describing his implementation of a specific specification.</xs:documentation>
			<xs:appinfo>Definition in IEC 61850-90-30</xs:appinfo>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="eIEC61850-6-100:tBaseExtensionElementWithDesc">
				<xs:attribute name="processReference" type="eIEC61850-6-100:tPathName" use="optional">
					<xs:annotation>
						<xs:documentation>path to an element in the process belonging to this project</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="processUuid" type="scl:tUUIDAttribute" use="optional">
					<xs:annotation>
						<xs:documentation>UUID of an element in the process belonging to this project</xs:documentation>
					</xs:annotation>
				</xs:attribute>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="tProject">
		<xs:annotation>
			<xs:documentation>Definition of the Project element</xs:documentation>
			<xs:appinfo>Definition in IEC 61850-90-30</xs:appinfo>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="eIEC61850-6-100:tBaseExtensionElementWithDesc">
				<xs:sequence>
					<xs:element name="ProjectProcessReference" type="eIEC61850-6-100:tProjectProcessReference" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>Reference to the process elements belonging to the current project</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
				<xs:attribute name="name" use="required">
					<xs:annotation>
						<xs:documentation>Name of the Project</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:normalizedString">
							<xs:minLength value="1"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:attribute>
				<xs:attribute name="uuid" type="scl:tUUIDAttribute">
					<xs:annotation>
						<xs:documentation>UUID identifying uniquelly the project</xs:documentation>
					</xs:annotation>
				</xs:attribute>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:element name="Project" type="eIEC61850-6-100:tProject">
		<xs:annotation>
			<xs:documentation>Element allowing to define a project, to identify the project and the process elements defined by this project</xs:documentation>
			<xs:appinfo>Definition in IEC 61850-90-30</xs:appinfo>
		</xs:annotation>
	</xs:element>
	<!-- ******************************** -->
	<!-- Function template Definition -->
	<!-- ******************************** -->
	<xs:complexType name="t6-100LNodeContainer" abstract="true">
		<xs:complexContent>
			<xs:extension base="eIEC61850-6-100:tBaseExtensionElementWithDesc">
				<xs:sequence>
					<xs:element name="LNode" type="scl:tLNode" minOccurs="0" maxOccurs="unbounded"/>
				</xs:sequence>
				<xs:attribute name="name" type="scl:tName" use="required"/>
				<xs:attributeGroup ref="eIEC61850-6-100:agUuidWithOrigin"/>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="tFunctionTemplate">
		<xs:complexContent>
			<xs:extension base="eIEC61850-6-100:t6-100LNodeContainer">
				<xs:sequence>
					<xs:element name="SubFunctionTemplate" type="eIEC61850-6-100:tSubFunctionTemplate" minOccurs="0" maxOccurs="unbounded">
						<xs:unique name="uniqueLNodeInSubFunctionTemplate">
							<xs:selector xpath="./scl:LNode"/>
							<xs:field xpath="@lnInst"/>
							<xs:field xpath="@lnClass"/>
							<xs:field xpath="@iedName"/>
							<xs:field xpath="@ldInst"/>
							<xs:field xpath="@prefix"/>
						</xs:unique>
						<xs:unique name="uniqueChildNameInSubFuncTemplate">
							<xs:selector xpath="./*"/>
							<xs:field xpath="@name"/>
						</xs:unique>
					</xs:element>
					<xs:element name="GeneralEquipment" type="scl:tGeneralEquipment" minOccurs="0" maxOccurs="unbounded">
						<xs:unique name="uniqueLNodeInGeneralEquipmentOfFunctionTemplate">
							<xs:selector xpath="./scl:LNode"/>
							<xs:field xpath="@lnInst"/>
							<xs:field xpath="@lnClass"/>
							<xs:field xpath="@iedName"/>
							<xs:field xpath="@ldInst"/>
							<xs:field xpath="@prefix"/>
						</xs:unique>
						<xs:unique name="uniqueChildNameInGEFuncTemplate">
							<xs:selector xpath="./*"/>
							<xs:field xpath="@name"/>
