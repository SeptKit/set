	</xs:complexType>
	<xs:complexType name="tL3IPv4CommParameters">
		<xs:annotation>
			<xs:documentation>Communication parameters for layer 3 (routable) Goose and SMV service specification</xs:documentation>
			<xs:appinfo>Definition in IEC 61850-90-30</xs:appinfo>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="eIEC61850-6-100:tCommParameters">
