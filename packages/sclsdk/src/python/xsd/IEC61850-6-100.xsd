					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="multicast" type="xs:boolean" use="optional">
					<xs:annotation>
						<xs:documentation>value to use for the multicast</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="smpRate" type="xs:unsignedInt" use="optional">
