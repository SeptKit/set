			<xs:appinfo>Definition in IEC 61850-90-30</xs:appinfo>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="eIEC61850-6-100:tWiringParameters">
				<xs:attribute name="debTm" type="xs:integer" use="optional">
					<xs:annotation>
						<xs:documentation>Debounce Time to use in LPDI (in ms)</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="vInOff" type="xs:float" use="optional">
					<xs:annotation>
						<xs:documentation>Drop-off voltage to use in LPDI</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="vInOn" type="xs:float" use="optional">
					<xs:annotation>
						<xs:documentation>Pick-up voltage to use in LPDI</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="outRef" type="xs:normalizedString" use="optional">
					<xs:annotation>
						<xs:documentation>Reference of the output to use in LPDO</xs:documentation>
					</xs:annotation>
