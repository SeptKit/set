						<xs:documentation>MAC-Address to be used on implementation</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:normalizedString">
							<xs:pattern value="[0-9A-F]{2}\-[0-9A-F]{2}\-[0-9A-F]{2}\-[0-9A-F]{2}\-[0-9A-F]{2}\-[0-9A-F]{2}"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:attribute>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="tL3IPv6CommParameters">
		<xs:annotation>
			<xs:documentation>Communication parameters for layer 3 (routable) Goose and SMV service specification</xs:documentation>
			<xs:appinfo>Definition in IEC 61850-90-30</xs:appinfo>
