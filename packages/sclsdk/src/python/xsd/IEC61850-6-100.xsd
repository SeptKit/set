			<xs:documentation>Communication parameters for Goose and SMV service specification</xs:documentation>
			<xs:appinfo>Definition in IEC 61850-90-30</xs:appinfo>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="eIEC61850-6-100:tBaseExtensionElementWithDesc">
				<xs:attribute name="vlanId" use="optional">
					<xs:annotation>
						<xs:documentation>VLAN ID to be used on implementation</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:normalizedString">
							<xs:pattern value="[0-9A-F]{3}"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:attribute>
				<xs:attribute name="vlanPriority" use="optional">
					<xs:annotation>
						<xs:documentation>VLAN Priority to be used on implementation</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:normalizedString">
							<xs:pattern value="[0-7]"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:attribute>
				<xs:attribute name="appId" use="optional">
					<xs:annotation>
						<xs:documentation>APPID to be used on implementation</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:normalizedString">
							<xs:pattern value="[0-9A-F]{4}"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:attribute>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="tL2CommParameters">
		<xs:annotation>
			<xs:documentation>Communication parameters for layer 2 (non routable) Goose and SMV service specification</xs:documentation>
			<xs:appinfo>Definition in IEC 61850-90-30</xs:appinfo>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="eIEC61850-6-100:tCommParameters">
				<xs:attribute name="macAddr" use="optional">
