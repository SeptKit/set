						<xs:documentation>Physical output type to use in LPDO</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:normalizedString">
							<xs:enumeration value="Normally open"/>
							<xs:enumeration value="Normally closed"/>
							<xs:enumeration value="Change over"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:attribute>
				<xs:attribute name="fastOutput" type="xs:boolean" use="optional">
					<xs:annotation>
						<xs:documentation>Indicates if the output needs to be a fast output in LPDO</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="outOffDl" type="xs:float" use="optional">
					<xs:annotation>
						<xs:documentation>Delay of the output (drop-off) to use in LPDO</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="outOnDl" type="xs:float" use="optional">
					<xs:annotation>
						<xs:documentation>Delay of the output (pick-up) to use in LPDO</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="outNam" type="xs:normalizedString" use="optional">
					<xs:annotation>
						<xs:documentation>Label text of the output to use in LPDO</xs:documentation>
					</xs:annotation>
				</xs:attribute>
			</xs:extension>
		</xs:complexContent>
