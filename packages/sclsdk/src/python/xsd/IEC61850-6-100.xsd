	<xs:complexType name="tGooseParameters">
		<xs:annotation>
			<xs:documentation>GOOSE parameters for service specification</xs:documentation>
			<xs:appinfo>Definition in IEC 61850-90-30</xs:appinfo>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="eIEC61850-6-100:tControlBlockParameters">
				<xs:choice minOccurs="0" maxOccurs="1">
					<xs:element name="L2CommParameters" type="eIEC61850-6-100:tL2CommParameters"/>
					<xs:element name="L3IPv4CommParameters" type="eIEC61850-6-100:tL3IPv4CommParameters"/>
					<xs:element name="L3IPv6CommParameters" type="eIEC61850-6-100:tL3IPv6CommParameters"/>
				</xs:choice>
				<xs:attribute name="goId" type="xs:normalizedString" use="optional">
					<xs:annotation>
						<xs:documentation>GOOSE identifier to be used</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="securityEnabled" type="xs:boolean" use="optional">
					<xs:annotation>
						<xs:documentation>indicates if the security is enable. The choice of kind of security is done based on supported security</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="minTime" type="xs:decimal" use="optional">
					<xs:annotation>
						<xs:documentation>value to use for the minTime</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="maxTime" type="xs:decimal" use="optional">
					<xs:annotation>
						<xs:documentation>value to use for the maxTime</xs:documentation>
					</xs:annotation>
				</xs:attribute>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="tSMVParameters">
